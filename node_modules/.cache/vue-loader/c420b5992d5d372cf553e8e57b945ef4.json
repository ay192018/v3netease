-model:active=\"active\" animated shrink swipeable>\r\n      <van-tab :title=\"item.name\" v-for=\"(item, index) in songs\" :key=\"index\">\r\n        <div class=\"content\">\r\n          <van-swipe class=\"my-swipe\" indicator-color=\"white\" :show-indicators=\"false\" :width=\"325\" :loop=\"false\">\r\n            <van-swipe-item v-for=\"(swipe, indexs) in item.sing\" :key=\"indexs\">\r\n              <div class=\"item\" v-for=\"(itemsinger, indexx) in swipe.resources\" :key=\"indexx\">\r\n                <div class=\"left\">\r\n                  <div class=\"img\">\r\n                    <van-image width=\"40\" height=\"40\" fit=\"cover\" radius=\"15\" />\r\n                  </div>\r\n\r\n                  <div class=\"songdata\">\r\n                    <div class=\"name van-ellipsis\">\r\n                      <span class=\"singer van-ellipsis\">-{{ itemsinger.resourceExtInfo.songData }}</span>\r\n                    </div>\r\n                    <div class=\"singer\"></div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </van-swipe-item>\r\n          </van-swipe></div\r\n      ></van-tab>\r\n    </van-tabs>\r\n    <div class=\"recommend \">\r\n      <van-button type=\"primary\" class=\"btn\" hairline round size=\"mini\">\r\n        <template #icon> 更多<van-icon name=\"arrow\" /> </template>\r\n      </van-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed, ref, watchPostEffect } from 'vue';\r\nexport default {\r\n  setup(props, { attrs }) {\r\n    const active = ref(0);\r\n    const songs = ref([]);\r\n\r\n    watchPostEffect(() => {\r\n      console.log(attrs.NewSongs);\r\n      const list = [\r\n        {\r\n          name: '',\r\n          sing: [],\r\n        },\r\n        {\r\n          name: '',\r\n          sing: [],\r\n        },\r\n        {\r\n          name: '',\r\n          sing: [],\r\n        },\r\n      ];\r\n\r\n      Array.from(\r\n        new Set(\r\n          attrs.NewSongs.map((item, index) => {\r\n            return item.uiElement.mainTitle.title;\r\n          }),\r\n        ),\r\n      ).forEach((title, index) => {\r\n        list[index].name = title;\r\n      });\r\n      attrs.NewSongs.map((item) => {\r\n        list.forEach((title) => {\r\n          if (item.uiElement.mainTitle.title === title.name) {\r\n            title.sing.push(item);\r\n          }\r\n        });\r\n      });\r\n\r\n      songs.value = list;\r\n      console.log(songs.value);\r\n    });\r\n\r\n    return {\r\n      active,\r\n      songs,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.Random {\r\n  position: relative;\r\n  .recommend {\r\n    position: absolute;\r\n    top: 5px;\r\n    right: 3px;\r\n    .btn {\r\n      width: 50px;\r\n    }\r\n  }\r\n  .content {\r\n    width: auto;\r\n    height: auto;\r\n    padding: 5px;\r\n    .item {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      align-items: center;\r\n      margin: 5px 0;\r\n      .left {\r\n        display: flex;\r\n        align-items: left;\r\n        .img {\r\n          margin-right: 5px;\r\n          position: relative;\r\n          .icon {\r\n            position: absolute;\r\n            left: 27%;\r\n            top: 27%;\r\n            z-index: 1;\r\n          }\r\n        }\r\n        .songdata {\r\n          display: flex;\r\n          flex-direction: column;\r\n          justify-content: space-around;\r\n          font-size: 13px;\r\n          width: 250px;\r\n          .name {\r\n            text-align: left !important;\r\n          }\r\n          .singer {\r\n            color: #c8c9cc;\r\n            text-align: left !important;\r\n          }\r\n        }\r\n      }\r\n      .play {\r\n        margin-right: 15px;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}