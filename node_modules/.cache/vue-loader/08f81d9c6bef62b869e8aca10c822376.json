 onMounted(async () => {\n      const { data } = await getindexFind();\n      // console.log(data.data);\n      icons.icon = markRaw(data.data);\n    });\n    const torouter = (index) => {\n      if (index === 3) {\n        return router.push('/ranking');\n      }\n      Notify({ type: 'warning', message: '功能暂未开发' });\n    };\n    return {\n      icons,\n      week,\n      Notify,\n      torouter,\n      router,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.icons {\n  overflow: auto;\n  display: flex;\n  .item {\n    position: relative;\n    margin-right: 15px;\n    text-align: center;\n    img {\n      margin-bottom: 5px;\n      width: 50px;\n      height: 50px;\n      border-radius: 50%;\n      background: red;\n      background-size: cover;\n    }\n  }\n  .data {\n    font-size: 15px;\n    font-weight: bold;\n    color: red;\n    position: absolute;\n    left: 50%;\n    top: 40%;\n    transform: translate(-50%, -50%);\n  }\n}\n</style>\n\n<template>\n  <div class=\"icons auto\">\n    <div class=\"item\" v-for=\"(item, index) in icons.icon\" :key=\"item.id\" @click=\"torouter(index)\">\n      <img :src=\"item.iconUrl\" alt=\"\" />\n      <span class=\"title\">{{ item.name }}</span>\n      <div v-if=\"index === 0\" class=\"data\">\n        {{ week(Date.parse(new Date())) }}\n      </div>\n    </div>\n  </div>\n</template>\n"]}]}