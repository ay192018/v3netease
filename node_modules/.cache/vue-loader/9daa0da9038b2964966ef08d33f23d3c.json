       icon=\"revoke\"\r\n        round\r\n        size=\"mini\"\r\n        @click=\"replist\"\r\n        >换一批</van-button\r\n      >\r\n    </div>\r\n    <div class=\"videolist\">\r\n      <div class=\"item\" v-for=\"(item, index) in videolist.list\" :key=\"index\">\r\n        <van-image\r\n          width=\"100\"\r\n          height=\"130\"\r\n          radius=\"15\"\r\n          fit=\"cover\"\r\n          :src=\"item.data.coverUrl\"\r\n          @click=\"\r\n            router.push({\r\n              name: 'video',\r\n              params: {\r\n                id: item.data.vid,\r\n              },\r\n            })\r\n          \"\r\n        >\r\n          <template v-slot:loading>\r\n            <van-loading type=\"spinner\" size=\"20\" color=\"#000\" />\r\n          </template>\r\n        </van-image>\r\n        <div class=\"titles van-multi-ellipsis--l2\">\r\n          {{ item.data.title }}\r\n        </div>\r\n        <span class=\"count\">{{ playCount(item.data.playTime) }}</span>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { playCount } from \"@/Util/fltter.js\";\r\nimport { onMounted, reactive } from \"@vue/runtime-core\";\r\nimport { getvideo } from \"@/api/video.js\";\r\nimport { useRouter } from \"vue-router\";\r\nexport default {\r\n  setup() {\r\n    const router = useRouter();\r\n    const videolist = reactive({\r\n      list: [],\r\n      offset: 0,\r\n    });\r\n    onMounted(async () => {\r\n      const { data } = await getvideo({\r\n        offset: videolist.offset,\r\n      });\r\n      // console.log(data);\r\n      videolist.list = data.datas;\r\n    });\r\n    const replist = async () => {\r\n      const { data } = await getvideo({\r\n        offset: videolist.offset++,\r\n      });\r\n      // console.log(data);\r\n      videolist.list = data.datas;\r\n    };\r\n    return {\r\n      videolist,\r\n      playCount,\r\n      replist,\r\n      router,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.video {\r\n  padding-bottom: 200px;\r\n  height: auto;\r\n\r\n  .title {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n  .videolist {\r\n    overflow-x: auto;\r\n    overflow-y: hidden;\r\n    height: auto;\r\n    display: flex;\r\n    .item {\r\n      width: 100px;\r\n      position: relative;\r\n      margin-right: 15px;\r\n      height: auto;\r\n      .titles {\r\n        height: auto;\r\n        width: 100%;\r\n        overflow: hidden;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}