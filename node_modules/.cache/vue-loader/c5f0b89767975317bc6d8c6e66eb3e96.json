r } from \"vue-router\";\nexport default {\n  setup() {\n    const songsheet = reactive({ songs: [] });\n    onMounted(async () => {\n      const { data } = await getrecommendlist({ limit: 30 });\n      songsheet.songs = data.result;\n      //   console.log(songsheet);\n    });\n    const DynamicRouter = useRouter();\n\n    const router = (id) => {\n      DynamicRouter.push({\n        name: \"songsdetails\",\n        params: { id: id },\n      });\n    };\n\n    return {\n      songsheet,\n      playCount,\n      router,\n      DynamicRouter,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.Recommendlist {\n  overflow-x: auto;\n  height: 150px;\n\n  display: flex;\n  .item {\n    width: 110px;\n    position: relative;\n    margin-right: 15px;\n    .title {\n      height: auto;\n      width: 100%;\n    }\n  }\n}\n</style>\n"]}]}