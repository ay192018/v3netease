AE;YACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAChC;QACF,CAAC,CAAC;QACF,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACvB,EAAE,CAAC,CAAC,CAAC,EAAE;QACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACR;IACF,CAAC,CAAC;;IAEF,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;MACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACN,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACR,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACN,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACR,CAAC;EACH,CAAC;AACH,CAAC","file":"D:/HTML工程/项目/v3netease-main/src/views/my/Userplaylist.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"Userplaylist\">\n    <div\n      class=\"like\"\n      @click=\"\n        router.push({\n          name: 'songsdetails',\n          params: {\n            id: playlist.like.id,\n          },\n        })\n      \"\n    >\n      <div class=\"left\">\n        <van-image\n          width=\"40\"\n          height=\"40\"\n          radius=\"15\"\n          :src=\"playlist.like.coverImgUrl\"\n        />\n        <div class=\"describe\">\n          <span class=\"name\">{{ playlist.like.name }}</span\n          ><br />\n          <span class=\"trackCount\"\n            >{{ cookie ? playlist.like.trackCount : 0 }} 首</span\n          >\n        </div>\n      </div>\n      <van-button round size=\"mini\" icon=\"like-o\" class=\"btn\"\n        >心动模式</van-button\n      >\n    </div>\n\n    <van-tabs v-model:active=\"active\" scrollspy sticky>\n      <van-tab :title=\"playlist.user ? '创建歌单' : ''\">\n        <div class=\"ceated ceateds\" v-if=\"cookie\">\n          <div class=\"trackCount gedan\">\n            创建歌单({{ playlist.user.length }}个)\n          </div>\n          <div\n            class=\"item\"\n            v-for=\"(item, index) in playlist.user\"\n            :key=\"index\"\n            @click=\"\n              router.push({\n                name: 'songsdetails',\n                params: {\n                  id: item.id,\n                },\n              })\n            \"\n          >\n            <van-image\n              class=\"itemimg\"\n              width=\"40\"\n              height=\"40\"\n              radius=\"15\"\n              :src=\"item.coverImgUrl\"\n            />\n            <div class=\"describe\">\n              <span class=\"name names\">{{ item.name }}</span\n              ><br />\n              <span class=\"trackCount\"\n                >{{ item.trackCount }} 首 {{ item.creator.nickname }}\n              </span>\n              <div class=\"van-hairline--bottom\"></div>\n            </div>\n          </div>\n        </div>\n        <van-empty v-else />\n      </van-tab>\n\n      <van-tab :title=\"playlist.collection ? '收藏歌单' : ''\">\n        <div class=\"ceated ceateds padding\" v-if=\"cookie\">\n          <div class=\"trackCount gedan\">\n            收藏歌单 ({{ playlist.collection.length }}个)\n          </div>\n          <div\n            class=\"item\"\n            v-for=\"(item, index) in playlist.collection\"\n            :key=\"index\"\n            @click=\"\n              router.push({\n                name: 'songsdetails',\n                params: {\n                  id: item.id,\n                },\n              })\n            \"\n          >\n            <van-image\n              class=\"itemimg\"\n              width=\"40\"\n              height=\"40\"\n              radius=\"15\"\n              :src=\"item.coverImgUrl\"\n            />\n            <div class=\"describe\">\n              <span class=\"name names\">{{ item.name }}</span\n              ><br />\n              <span class=\"trackCount\"\n                >{{ item.trackCount }} 首 {{ item.creator.nickname }}</span\n              >\n            </div>\n          </div>\n        </div>\n        <van-empty v-else />\n      </van-tab>\n    </van-tabs>\n  </div>\n</template>\n\n<script>\nimport { getuserplaylist } from \"@/api/user.js\";\nimport { onMounted, reactive, ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\nexport default {\n  setup() {\n    const active = ref(0);\n    const playlist = reactive({\n      user: [],\n      collection: [],\n      like: [],\n    });\n\n    const cookie = JSON.parse(localStorage.getItem(\"cookie\"));\n    const router = useRouter();\n    onMounted(async () => {\n      if (cookie) {\n        const { data } = await getuserplaylist({\n          uid: JSON.parse(localStorage.getItem(\"profile\")).userId,\n        });\n\n        playlist.like = data.playlist[0];\n        data.playlist.filter((item) => {\n          if (\n            item.creator.nickname ===\n            JSON.parse(localStorage.getItem(\"profile\")).nickname\n          ) {\n            playlist.user.push(item);\n          } else {\n            playlist.collection.push(item);\n          }\n        });\n        playlist.user.shift();\n      } else {\n        return;\n      }\n    });\n\n    return {\n      active,\n      playlist,\n      router,\n      cookie,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.Userplaylist {\n  width: 95vw;\n  margin: 20px auto;\n  .like {\n    width: 100%;\n    height: 10vh;\n    margin: 20px auto;\n    background: #fefefe;\n    border-radius: 15px;\n    display: flex;\n    justify-content: space-around;\n    align-items: center;\n    .left {\n      display: flex;\n      align-items: center;\n      margin-left: -30px;\n    }\n    .btn {\n      margin-right: -30px;\n    }\n  }\n  /deep/ .van-tabs__wrap {\n    border-radius: 15px;\n  }\n  /deep/ .van-tabs__nav {\n    background: transparent;\n  }\n\n  .ceated {\n    width: 95vw;\n    margin: 0 auto;\n    height: auto;\n    border-radius: 15px;\n    background: #fefefe;\n    .item {\n      width: 100%;\n      display: flex;\n      padding: 5px 0;\n      .itemimg {\n        margin-left: 15px;\n      }\n    }\n  }\n  .describe {\n    display: flex;\n    flex-direction: column;\n    justify-content: space-around;\n    margin-left: 10px;\n    .name {\n      font-size: 12px;\n    }\n  }\n  .ceateds {\n    margin-top: 15px;\n    width: 95vw;\n    background: #fefefe;\n\n    height: auto;\n  }\n  .names {\n    width: 60vw;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n    overflow: hidden;\n  }\n  .trackCount {\n    font-size: 8px;\n    color: #bfbfbf;\n  }\n  .gedan {\n    width: 94%;\n    margin: 5px auto;\n    padding: 5px;\n  }\n  /deep/ .van-empty {\n    height: 20vh;\n  }\n  .padding {\n    padding-bottom: 200px;\n  }\n}\n</style>\n"]}]}