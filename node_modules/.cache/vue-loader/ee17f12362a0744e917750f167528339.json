o.js';\r\n\r\nimport { ref, reactive } from 'vue';\r\nimport { playCount } from '@/Util/fltter.js';\r\nimport { useRouter } from 'vue-router';\r\n\r\nexport default {\r\n  name: 'itemvideo',\r\n  setup(props, { attrs }) {\r\n    const loading = ref(false);\r\n    const finished = ref(false);\r\n    const videolist = reactive({\r\n      datas: [],\r\n      offset: 0,\r\n    });\r\n\r\n    const router = useRouter();\r\n\r\n    const onLoad = async () => {\r\n      const { data } = await getgroup({\r\n        id: attrs.item,\r\n        offset: videolist.offset,\r\n      });\r\n\r\n      videolist.datas.push(...data.datas);\r\n      loading.value = false;\r\n\r\n      if (data.datas.length) {\r\n        videolist.offset++;\r\n      } else {\r\n        finished.value = true;\r\n      }\r\n    };\r\n\r\n    return {\r\n      loading,\r\n      finished,\r\n      onLoad,\r\n      videolist,\r\n      playCount,\r\n      router,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.item {\r\n  margin: 0 auto;\r\n  height: 78vh;\r\n\r\n  overflow-y: auto;\r\n  .videoitem {\r\n    width: 47%;\r\n    height: auto;\r\n\r\n    border-radius: 15px;\r\n    margin: 5px;\r\n    .title {\r\n      margin: 5px;\r\n    }\r\n    .user {\r\n      width: 95%;\r\n      margin: 0 auto;\r\n\r\n      display: flex;\r\n      align-items: center;\r\n      & div {\r\n      }\r\n      .title {\r\n        width: 35%;\r\n      }\r\n    }\r\n  }\r\n  ::v-deep(.van-list) {\r\n    display: flex;\r\n    justify-content: space-around;\r\n    flex-wrap: wrap;\r\n    padding-bottom: 50px;\r\n  }\r\n  .job {\r\n    text-overflow: ellipsis;\r\n    white-space: nowrap;\r\n    overflow: hidden;\r\n  }\r\n}\r\n</style>\r\n\r\n<template>\r\n  <div class=\"item\">\r\n    <van-list v-model:loading=\"loading\" :finished=\"finished\" finished-text=\"没有更多了\" @load=\"onLoad\">\r\n      <div class=\"videoitem\" v-for=\"(item, index) in videolist.datas\" :key=\"index\">\r\n        <div>\r\n          <van-image\r\n            width=\"100%\"\r\n            fit=\"cover\"\r\n            radius=\"15\"\r\n            :src=\"item.data.coverUrl\"\r\n            @click=\"\r\n              router.push({\r\n                name: 'video',\r\n                params: {\r\n                  id: item.data.vid,\r\n                },\r\n              })\r\n            \"\r\n          >\r\n            <template v-slot:loading> <van-loading type=\"spinner\" size=\"20\" color=\"#000\" /> </template\r\n          ></van-image>\r\n        </div>\r\n        <div class=\"title van-multi-ellipsis--l2\">\r\n          {{ item.data.title }}\r\n        </div>\r\n        <div class=\"user\">\r\n          <div>\r\n            <van-image width=\"30\" height=\"30\" fit=\"cover\" round :src=\"item.data.creator.avatarUrl\" />\r\n          </div>\r\n          <div class=\"title van-ellipsis\">\r\n            {{ item.data.creator.nickname }}\r\n          </div>\r\n          <div class=\"job\">\r\n            <van-icon name=\"good-job-o\" /><span>{{ playCount(item.data.playTime) }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <nut-backtop :bottom=\"150\" :right=\"50\"></nut-backtop>\r\n    </van-list>\r\n  </div>\r\n</template>\r\n"]}]}