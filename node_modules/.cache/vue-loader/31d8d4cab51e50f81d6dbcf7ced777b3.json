AAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACvB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;MAEnB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;IACb;IACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;MACR,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;MACf,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IAChB;EACF;AACF","file":"D:/HTML工程/项目/v3netease-main/src/views/search/playlist.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"playlist\">\n    <van-list\n      v-model:loading=\"loading\"\n      :finished=\"finished\"\n      finished-text=\"没有更多了\"\n      @load=\"onLoad\"\n    >\n      <van-cell\n        v-for=\"(item, index) in album\"\n        :key=\"index\"\n        clickable\n        @click=\"\n          router.push({\n            name: 'songsdetails',\n            params: {\n              id: item.id,\n            },\n          })\n        \"\n      >\n        <template #icon>\n          <div>\n            <van-image\n              width=\"50\"\n              height=\"50\"\n              radius=\"15\"\n              :src=\"item.coverImgUrl\"\n            />\n          </div>\n        </template>\n        <template #title>\n          <div class=\"content\">\n            <div class=\"title\" style=\"overflow: hidden\">{{ item.name }}</div>\n            <div class=\"describe\">\n              {{\n                `${item.trackCount}首音乐  by${\n                  item.creator.nickname\n                } 播放${playCount(item.playCount)}次`\n              }}\n            </div>\n          </div>\n        </template>\n      </van-cell>\n    </van-list>\n  </div>\n</template>\n\n<script>\nimport { getcloudsearch } from \"@/api/search.js\";\nimport { playCount } from \"@/Util/fltter.js\";\nimport { ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\n\nexport default {\n  setup(props, { attrs }) {\n    const album = ref([]);\n    const loading = ref(false);\n    const finished = ref(false);\n    const router = useRouter();\n    const offset = ref(1);\n    const onLoad = async () => {\n      console.log(attrs.attrs.value);\n      const { data } = await getcloudsearch({\n        keywords: attrs.attrs.value,\n        type: 1000,\n        offset: (offset.value - 1) * 30,\n      });\n      console.log(data);\n      album.value.push(...data.result.playlists);\n      loading.value = false;\n      if (data.result.playlists.length) {\n        offset.value++;\n      } else {\n        finished.value = true;\n      }\n    };\n\n    return {\n      attrs,\n      album,\n      onLoad,\n      loading,\n      finished,\n      router,\n      playCount,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.playlist {\n  background: #fff;\n  padding-bottom: 20px;\n  height: 80vh;\n  overflow-y: auto;\n  .content {\n    margin-left: 15px;\n    .title {\n      margin-right: 4px;\n      vertical-align: middle;\n      text-overflow: ellipsis;\n      white-space: nowrap;\n\n      width: 80vw;\n    }\n    .describe {\n      font-size: 13px;\n      color: #dedede;\n    }\n  }\n}\n</style>\n"]}]}