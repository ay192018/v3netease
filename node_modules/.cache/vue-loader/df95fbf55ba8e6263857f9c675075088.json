inner\" size=\"20\" color=\"#000\" />\r\n          </template>\r\n        </van-image>\r\n        <div class=\"titles van-multi-ellipsis--l2\">\r\n          {{ item.data.title }}\r\n        </div>\r\n        <span class=\"count\">{{ playCount(item.data.playTime) }}</span>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <van-empty image=\"network\" description=\"请登录\" v-else>\r\n    <van-button\r\n      round\r\n      type=\"danger\"\r\n      class=\"bottom-button\"\r\n      @click=\"router.push('/login')\"\r\n      >登录</van-button\r\n    >\r\n  </van-empty>\r\n</template>\r\n\r\n<script>\r\nimport { playCount } from \"@/Util/fltter.js\";\r\nimport { onMounted, reactive } from \"@vue/runtime-core\";\r\nimport { getvideo } from \"@/api/video.js\";\r\nimport { useRouter } from \"vue-router\";\r\nimport { ref } from \"vue\";\r\nexport default {\r\n  setup() {\r\n    const router = useRouter();\r\n    const videolist = reactive({\r\n      list: [],\r\n      offset: 0,\r\n    });\r\n    const cookie = ref(JSON.parse(window.localStorage.getItem(\"cookie\")));\r\n    onMounted(async () => {\r\n      const { data } = await getvideo({\r\n        offset: videolist.offset,\r\n      });\r\n      // console.log(data);\r\n      videolist.list = data.datas;\r\n    });\r\n    const replist = async () => {\r\n      const { data } = await getvideo({\r\n        offset: videolist.offset++,\r\n      });\r\n      // console.log(data);\r\n      videolist.list = data.datas;\r\n    };\r\n    return {\r\n      videolist,\r\n      playCount,\r\n      replist,\r\n      router,\r\n      ref,\r\n      cookie,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.video {\r\n  padding-bottom: 200px;\r\n  height: auto;\r\n\r\n  .title {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n  .videolist {\r\n    overflow-x: auto;\r\n    overflow-y: hidden;\r\n    height: auto;\r\n    display: flex;\r\n    .item {\r\n      width: 100px;\r\n      position: relative;\r\n      margin-right: 15px;\r\n      height: auto;\r\n      .titles {\r\n        height: auto;\r\n        width: 100%;\r\n        overflow: hidden;\r\n      }\r\n    }\r\n  }\r\n}\r\n.bottom-button {\r\n  width: 160px;\r\n  height: 40px;\r\n}\r\n</style>\r\n"]}]}