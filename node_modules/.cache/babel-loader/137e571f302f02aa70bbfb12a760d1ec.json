FJ,SAAD,CAAN;AAIF,OANA,MAMO,IAAI,KAAK,CAAC,KAAN,CAAY,SAAZ,KAA0B,CAA9B,EAAiC;AACtC,QAAA,KAAK,CAAC,QAAN,CAAe,cAAf,EAA+B,CAA/B;AACA,QAAA,KAAK,CAAC,KAAN,CAAY,KAAZ,CAAkB,IAAlB,GAAyB,IAAzB;AACA,QAAA,MAAM,CAAC;AACL,UAAA,UAAU,EAAE,SADP;AAEL,UAAA,OAAO,EAAE;AAFJ,SAAD,CAAN;AAIF,OAPO,MAOA;AACL,QAAA,KAAK,CAAC,KAAN,CAAY,KAAZ,CAAkB,IAAlB,GAAyB,KAAzB;AACA,QAAA,KAAK,CAAC,QAAN,CAAe,cAAf,EAA+B,CAA/B;AACA,QAAA,MAAM,CAAC;AACL,UAAA,UAAU,EAAE,SADP;AAEL,UAAA,OAAO,EAAE;AAFJ,SAAD,CAAN;AAIF;AACD,KAtBD;;AAuBA,QAAM,YAAW,GAAI,SAAf,YAAe,GAAM;AACzB,MAAA,KAAK,CAAC,MAAD,CAAL;AACD,KAFD;;AAGA,QAAM,UAAS,GAAI,SAAb,UAAa,CAAC,GAAD,EAAS,CAC1B;AACA;AACD,KAHD;;AAIA,QAAM,QAAO,GAAI,SAAX,QAAW,CAAC,KAAD,EAAW;AAC1B,MAAA,KAAK,CAAC,KAAN,CAAY,KAAZ,CAAkB,WAAlB,GAAgC,KAAhC;AACD,KAFD;;AAIA;AACE,MAAA,SAAS,EAAT,SADF;AAEE,MAAA,UAAU,EAAV,UAFF;AAGE,MAAA,YAAY,EAAZ,YAHF;AAIE,MAAA,KAAK,EAAL,KAJF;AAKE,MAAA,KAAK,EAAL,KALF;AAME,MAAA,KAAK,EAAL,KANF;AAOE,MAAA,QAAQ,EAAR,QAPF;AAQE,MAAA,MAAM,EAAN;AARF,uCASE,KATF,8CAUE,gBAVF,wCAWE,UAXF,wCAYE,UAZF,wCAaE,UAbF,mCAcE,KAdF,sCAeE,QAfF,uCAgBE,SAhBF;AAkBD;AApIY,CAAf","sourcesContent":["<template>\n  <div class=\"info\">\n    <van-nav-bar\n      :border=\"false\"\n      :fixed=\"true\"\n      :title=\"`${store.state.songlist[store.state.curret].name}-${\n        store.state.songlist[store.state.curret].ar[0].name\n      }`\"\n      @click-left=\"changeshow\"\n      @click-right=\"onClickRight\"\n    >\n      <template #left>\n        <van-icon name=\"arrow-down\" color=\"#323233\" size=\"25\" />\n      </template>\n      <template #right>\n        <van-icon name=\"share-o\" size=\"25\" color=\"#323233\" /> </template\n    ></van-nav-bar>\n    <van-image\n      width=\"100vw\"\n      height=\"100vh\"\n      fit=\"cover\"\n      class=\"bgimg\"\n      :src=\"store.state.songlist[store.state.curret].al.picUrl\"\n    />\n    <van-image\n      width=\"180px\"\n      height=\"180px\"\n      round\n      fit=\"cover\"\n      class=\"rotateimg\"\n      :class=\"\n        store.state.audio.played && store.state.audio.readyState == 4\n          ? 'active'\n          : ''\n      \"\n      :src=\"store.state.songlist[store.state.curret].al.picUrl\"\n      v-show=\"!lyricsshow\"\n      @click=\"lyricsshow = !lyricsshow\"\n    >\n      <template v-slot:loading>\n        <van-loading type=\"spinner\" size=\"20\" color=\"#000\" /> </template\n    ></van-image>\n    <div class=\"lyrics\" v-show=\"lyricsshow\" @click=\"lyricsshow = !lyricsshow\">\n      <p\n        v-for=\"(item, index) in lyric.value\"\n        :key=\"index\"\n        :class=\"\n          attrs.currentTime >= lyric.key[index] &&\n          attrs.currentTime < lyric.key[index + 1]\n            ? 'actives   shake-constant shake-slow'\n            : ''\n        \"\n      >\n        {{ item[1] }}\n      </p>\n    </div>\n    <ul class=\"components\">\n      <li>\n        <van-icon name=\"like-o\" size=\"28\" color=\"rgb(50,50,51)\" /><van-icon\n          name=\"down\"\n          size=\"28\"\n          color=\"rgb(50,50,51)\"\n        /><van-icon\n          name=\"chat-o\"\n          size=\"28\"\n          color=\"rgb(50,50,51)\"\n          @click=\"tocommnts\"\n        /><van-icon name=\"ellipsis\" size=\"28\" color=\"rgb(50,50,51)\" />\n      </li>\n      <li>\n        <div class=\"time\">{{ realFormatSecond(attrs.currentTime) }}</div>\n        <van-slider\n          class=\"time\"\n          v-model=\"value\"\n          @change=\"onChange\"\n          active-color=\"red\"\n          button-size=\"12\"\n          :min=\"0\"\n          :max=\"attrs.duration\"\n          @update:model-value=\"modelvalue\"\n        />\n        <div class=\"time\">{{ realFormatSecond(attrs.duration) }}</div>\n      </li>\n      <li>\n        <van-icon\n          name=\"replay\"\n          size=\"28\"\n          color=\"rgb(50,50,51)\"\n          v-if=\"store.state.playmodel === 0\"\n          @click=\"playstyle\"\n        />\n        <van-icon\n          name=\"circle\"\n          size=\"28\"\n          color=\"rgb(50,50,51)\"\n          v-else-if=\"store.state.playmodel === 1\"\n          @click=\"playstyle\"\n        />\n        <van-icon\n          name=\"exchange\"\n          size=\"28\"\n          color=\"rgb(50,50,51)\"\n          v-else\n          @click=\"playstyle\"\n        />\n        <van-icon\n          name=\"arrow-left\"\n          size=\"28\"\n          color=\"rgb(50,50,51)\"\n          @click=\"switchsong(-1)\"\n        />\n        <van-icon\n          :name=\"store.state.isplay ? 'pause-circle-o' : 'play-circle-o'\"\n          size=\"32\"\n          color=\"rgb(50,50,51)\"\n          @click=\"attrs.play()\"\n        />\n        <van-icon\n          name=\"arrow\"\n          size=\"28\"\n          color=\"rgb(50,50,51)\"\n          @click=\"switchsong(1)\"\n        />\n        <van-icon name=\"bars\" size=\"28\" color=\"rgb(50,50,51)\" />\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport { getsonglyric } from \"@/api/songsheet.js\";\nimport { realFormatSecond, debounce, lyrics } from \"@/Util/fltter.js\";\nimport { nextTick, ref, watch, reactive } from \"vue\";\nimport { Toast, Notify } from \"vant\";\n\nimport { useStore } from \"vuex\";\nimport { useRouter } from \"vue-router\";\nexport default {\n  setup(props, { attrs, emit }) {\n    const store = useStore();\n    const value = ref(0);\n    const lyric = reactive({\n      value: null,\n      key: [],\n    });\n    const router = useRouter();\n    const lyricsshow = ref(false);\n    const tocommnts = () => {\n      store.dispatch(\"setflag\", 0);\n      router.push({\n        name: \"comments\",\n        params: {\n          id: store.state.songlist[store.state.curret].id,\n        },\n      });\n    };\n    const changeshow = () => {\n      //传递给父组件的弹出层状态\n      emit(\"changeshow\", !attrs.show);\n    };\n    watch(\n      //监听接收过来的当前播放时间来给绑定进度条的值进行赋值\n      () => attrs.currentTime,\n      () => {\n        value.value = Number(attrs.currentTime);\n\n        if (document.querySelector(\".lyrics\").scrollTop) {\n          setTimeout(() => {\n            document.querySelector(\".lyrics\").scrollTop =\n              document.querySelector(\".actives\").offsetTop - 150;\n          }, 1000);\n        }\n      }\n    );\n    watch(\n      /**\n       * @param {Object} 监听id索引的变化来请求当前id的歌词进行数据抽离\n       * @return {Promise} 数据\n       */\n\n      () => store.state.curret,\n      async (val) => {\n        const { data } = await getsonglyric({\n          id: store.state.songlist[val].id,\n        });\n\n        const result = lyrics(data);\n        // console.log(result);\n        lyric.value = result.result;\n        lyric.key = result.key;\n      },\n      { immediate: true }\n    );\n    /**\n     * @param {Number} 传入1||-1来决定上下首\n     *\n     */\n    const switchsong = (index) => {\n      if (store.state.songlist.length === store.state.curret + index) {\n        Notify({\n          background: \"#7232dd\",\n          message: \"没有下一首咯\",\n        });\n        return;\n      } else if (0 > store.state.curret + index) {\n        Notify({\n          background: \"#7232dd\",\n          message: \"没有上一首咯\",\n        });\n        documenM;\n        return;\n      }\n      store.dispatch(\"setsetcurret\", store.state.curret + index);\n      nextTick(() => {\n        store.state.audio.play();\n      });\n    };\n    const playstyle = () => {\n      if (store.state.playmodel === 0) {\n        store.dispatch(\"setplaymodel\", 1);\n        Notify({\n          background: \"pink\",\n          message: \"随机播放\",\n        });\n      } else if (store.state.playmodel === 1) {\n        store.dispatch(\"setplaymodel\", 2);\n        store.state.audio.loop = true;\n        Notify({\n          background: \"#ff6700\",\n          message: \"循环播放\",\n        });\n      } else {\n        store.state.audio.loop = false;\n        store.dispatch(\"setplaymodel\", 0);\n        Notify({\n          background: \"#ff6700\",\n          message: \"顺序播放\",\n        });\n      }\n    };\n    const onClickRight = () => {\n      Toast(\"分享事件\");\n    };\n    const modelvalue = (val) => {\n      //拖动实时更新进度条的start\n      // attrs.currentTime = val;\n    };\n    const onChange = (value) => {\n      store.state.audio.currentTime = value;\n    };\n\n    return {\n      playstyle,\n      changeshow,\n      onClickRight,\n      attrs,\n      store,\n      value,\n      onChange,\n      router,\n      attrs,\n      realFormatSecond,\n      switchsong,\n      modelvalue,\n      lyricsshow,\n      lyric,\n      debounce,\n      tocommnts,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.info {\n  width: 100vw;\n  height: 100vh;\n  position: relative;\n  /deep/ .van-nav-bar__title,\n  .van-ellipsis {\n    font-size: 12px;\n    color: gray;\n  }\n  /deep/ .van-nav-bar,\n  .van-nav-bar--fixed {\n    background: transparent;\n  }\n  .bgimg {\n    filter: blur(15px);\n    z-index: -99;\n  }\n  .rotateimg {\n    position: fixed;\n    left: 28%;\n    top: 28%;\n    transform: translate(-50%, -50%);\n    animation-name: Rotate;\n    animation-iteration-count: infinite;\n    animation-play-state: paused;\n    animation-timing-function: linear;\n    animation-duration: 16s;\n  }\n  .lyrics {\n    overflow-y: auto;\n    text-align: center;\n    position: fixed;\n    left: 10%;\n    top: 12%;\n    width: 80vw;\n    color: #f6f7f8;\n    height: 55vh;\n    scroll-behavior: smooth;\n    & {\n      font-size: 12px;\n    }\n  }\n  .actives {\n    transform: rotateX(360deg);\n    color: palevioletred;\n    text-shadow: 2px 2px 4px #fbd0db;\n    transition: all 0.5s;\n    font-size: 18px;\n    transition: all 0.5s;\n  }\n  .active {\n    animation-play-state: running;\n  }\n  @keyframes Rotate {\n    0% {\n      transform: rotate(0deg);\n    }\n    100% {\n      transform: rotate(360deg);\n    }\n  }\n  .components {\n    width: 100vw;\n    height: 35vh;\n\n    position: fixed;\n    left: 0;\n    right: 0;\n    bottom: 0;\n\n    li {\n      display: flex;\n      justify-content: space-around;\n      align-content: center;\n      width: 100%;\n      height: 33.3%;\n      text-align: center;\n\n      /deep/.van-badge__wrapper,\n      .van-icon,\n      .van-icon-down {\n        margin: auto 0;\n      }\n      /deep/ .van-slider {\n        width: 80vw;\n        margin: 0 20px;\n      }\n      .time {\n        margin: auto 0;\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}