ng\" :finished=\"finished\" finished-text=\"没有更多了\" @load=\"onLoad\">\n      <van-cell\n        v-for=\"(item, index) in album\"\n        :key=\"index\"\n        clickable\n        @click=\"\n          router.push({\n            name: 'songsdetails',\n            params: {\n              id: item.id,\n            },\n          })\n        \"\n      >\n        <template #icon>\n          <div>\n            <van-image width=\"50\" height=\"50\" radius=\"15\" :src=\"item.coverImgUrl\" />\n          </div>\n        </template>\n        <template #title>\n          <div class=\"content\">\n            <div class=\"title\" style=\"overflow: hidden\">{{ item.name }}</div>\n            <div class=\"describe\">\n              {{ `${item.trackCount}首音乐  by${item.creator.nickname} 播放${playCount(item.playCount)}次` }}\n            </div>\n          </div>\n        </template>\n      </van-cell>\n    </van-list>\n  </div>\n</template>\n"],"sourceRoot":""}]}