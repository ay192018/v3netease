=> {\n      console.log(attrs.attrs.value);\n      const { data } = await getcloudsearch({\n        keywords: attrs.attrs.value,\n        type: 1000,\n        offset: (offset.value - 1) * 30,\n      });\n      console.log(data);\n      album.value.push(...data.result.playlists);\n      loading.value = false;\n      if (data.result.playlists.length) {\n        offset.value++;\n      } else {\n        finished.value = true;\n      }\n    };\n\n    return {\n      attrs,\n      album,\n      onLoad,\n      loading,\n      finished,\n      router,\n      playCount,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.playlist {\n  background: #fff;\n  padding-bottom: 20px;\n  height: 80vh;\n  overflow-y: auto;\n  .content {\n    margin-left: 15px;\n    .title {\n      margin-right: 4px;\n      vertical-align: middle;\n      text-overflow: ellipsis;\n      white-space: nowrap;\n\n      width: 80vw;\n    }\n    .describe {\n      font-size: 13px;\n      color: #dedede;\n    }\n  }\n}\n</style>\n\n<template>\n  <div class=\"playlist\">\n    <van-list v-model:loading=\"loading\" :finished=\"finished\" finished-text=\"没有更多了\" @load=\"onLoad\">\n      <van-cell\n        v-for=\"(item, index) in album\"\n        :key=\"index\"\n        clickable\n        @click=\"\n          router.push({\n            name: 'songsdetails',\n            params: {\n              id: item.id,\n            },\n          })\n        \"\n      >\n        <template #icon>\n          <div>\n            <van-image width=\"50\" height=\"50\" radius=\"15\" :src=\"item.coverImgUrl\" />\n          </div>\n        </template>\n        <template #title>\n          <div class=\"content\">\n            <div class=\"title\" style=\"overflow: hidden\">{{ item.name }}</div>\n            <div class=\"describe\">\n              {{ `${item.trackCount}首音乐  by${item.creator.nickname} 播放${playCount(item.playCount)}次` }}\n            </div>\n          </div>\n        </template>\n      </van-cell>\n    </van-list>\n  </div>\n</template>\n"],"sourceRoot":""}]}