F;AAAA,aAAG,C,UAAA,CAAH;AAAA,K;;;;GAAxF,E;;AAAA,I,WAAA,CACI,CAHN,CA+BI,EA3BJ,mBAAA,CA0BM,KA1BN,EAAA,UAAA,EA0BM,E,kBAzBJ,mBAAA,CAwBM,SAxBN,EAwBM,IAxBN,EAwBM,WAAA,CAxBoC,MAAA,CAAA,SAAA,CAAU,IAwB9C,EAxBkD,UAA9B,IAA8B,EAAxB,KAAwB,EAAnB;yBAArC,mBAAA,CAwBM,KAxBN,EAwBM;AAxBD,MAAA,KAAK,EAAC,MAwBL;AAxBqD,MAAA,GAAG,EAAE;AAwB1D,KAxBN,E,CACE,YAAA,CAkBY,oBAlBZ,EAkBY;AAjBV,MAAA,KAAK,EAAC,KAiBI;AAhBV,MAAA,MAAM,EAAC,KAgBG;AAfV,MAAA,MAAM,EAAC,IAeG;AAdV,MAAA,GAAG,EAAC,OAcM;AAbT,MAAA,GAAG,EAAE,IAAI,CAAC,IAAL,CAAU,QAaN;AAZT,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAgB,MAAA,CAAA,MAAA,CAAO,IAAP,CAAW;uBAAA;;gBAA+E,IAAI,CAAC,IAAL,CAAU;;AAAzF,SAAX,CAAhB;AAAA;AAYI,KAlBZ,E;AAemB,MAAA,OAAO,EAAA,QAAA,CACtB;AAAA,eAAqD,CAArD,YAAA,CAAqD,sBAArD,EAAqD;AAAxC,UAAA,IAAI,EAAC,SAAmC;AAAzB,UAAA,IAAI,EAAC,IAAoB;AAAf,UAAA,KAAK,EAAC;AAAS,SAArD,CAAqD,CAArD;AAAA,OADsB,C;;;;KAf1B,E;;AAAA,M,kBAAA,C,EAmBA,mBAAA,CAEM,KAFN,EAAA,UAAA,EAEM,gBAAA,CADD,IAAI,CAAC,IAAL,CAAU,KACT,CAFN,EACoB;AAAA;AADpB,K,EAGA,mBAAA,CAA8D,MAA9D,EAAA,UAAA,EAA8D,gBAAA,CAAvC,MAAA,CAAA,SAAA,CAAU,IAAI,CAAC,IAAL,CAAU,QAApB,CAAuC,CAA9D,EAAmD;AAAA;AAAnD,K,CAvBF,C;GAwBM,CAxBN,E;;AAAA,GAyBI,EA1BN,CA2BI,CAhCN,C,EAiCA,mBAAA,CAAA,uPAAA,C","sourcesContent":["<script>\r\nimport { playCount } from '@/Util/fltter.js';\r\nimport { onMounted, reactive } from '@vue/runtime-core';\r\nimport { getvideo } from '@/api/video.js';\r\nimport { useRouter } from 'vue-router';\r\nimport { ref } from 'vue';\r\nexport default {\r\n  setup() {\r\n    const router = useRouter();\r\n    const videolist = reactive({\r\n      list: [],\r\n      offset: 0,\r\n    });\r\n    const cookie = ref(JSON.parse(window.localStorage.getItem('cookie')));\r\n    onMounted(async () => {\r\n      const { data } = await getvideo({\r\n        offset: videolist.offset,\r\n      });\r\n\r\n      videolist.list = data.datas;\r\n    });\r\n    const replist = async () => {\r\n      const { data } = await getvideo({\r\n        offset: videolist.offset++,\r\n      });\r\n      // console.log(data);\r\n      videolist.list = data.datas;\r\n    };\r\n    return {\r\n      videolist,\r\n      playCount,\r\n      replist,\r\n      router,\r\n      ref,\r\n      cookie,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.video {\r\n  padding-bottom: 200px;\r\n  height: auto;\r\n\r\n  .title {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n  }\r\n  .videolist {\r\n    overflow-x: auto;\r\n    overflow-y: hidden;\r\n    height: auto;\r\n    display: flex;\r\n    .item {\r\n      width: 100px;\r\n      position: relative;\r\n      margin-right: 15px;\r\n      height: auto;\r\n      .titles {\r\n        height: auto;\r\n        width: 100%;\r\n        overflow: hidden;\r\n      }\r\n    }\r\n  }\r\n}\r\n.bottom-button {\r\n  width: 160px;\r\n  height: 40px;\r\n}\r\n</style>\r\n\r\n<template>\r\n  <div class=\"video auto\">\r\n    <div class=\"title\">\r\n      <h3>精选音乐视频</h3>\r\n      <van-button type=\"primary\" color=\"red\" icon=\"revoke\" round size=\"mini\" @click=\"replist\">换一批</van-button>\r\n    </div>\r\n    <div class=\"videolist\">\r\n      <div class=\"item\" v-for=\"(item, index) in videolist.list\" :key=\"index\">\r\n        <van-image\r\n          width=\"100\"\r\n          height=\"130\"\r\n          radius=\"15\"\r\n          fit=\"cover\"\r\n          :src=\"item.data.coverUrl\"\r\n          @click=\"\r\n            router.push({\r\n              name: 'video',\r\n              params: {\r\n                id: item.data.vid,\r\n              },\r\n            })\r\n          \"\r\n        >\r\n          <template v-slot:loading>\r\n            <van-loading type=\"spinner\" size=\"20\" color=\"#000\" />\r\n          </template>\r\n        </van-image>\r\n        <div class=\"titles van-multi-ellipsis--l2\">\r\n          {{ item.data.title }}\r\n        </div>\r\n        <span class=\"count\">{{ playCount(item.data.playTime) }}</span>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <!--  <van-empty image=\"network\" description=\"请登录\" v-else>\r\n    <van-button\r\n      round\r\n      type=\"danger\"\r\n      class=\"bottom-button\"\r\n      @click=\"router.push('/login')\"\r\n      >登录</van-button\r\n    >\r\n  </van-empty> -->\r\n</template>\r\n"],"sourceRoot":""}]}