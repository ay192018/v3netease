C,IAAuD;AAAlD,cAAA,MAAM,EAAC,IAA2C;AAAtC,cAAA,MAAM,EAAC,IAA+B;AAAzB,cAAA,GAAG,EAAE,IAAI,CAAC;AAAe,aAAxF,E,IAAA,E,CAAA,E,OAAA,C,EACA,mBAAA,CAIM,KAJN,EAAA,WAAA,EAIM,CAHJ,mBAAA,CACC,MADD,EAAA,WAAA,EACC,gBAAA,CAD2B,IAAI,CAAC,IAChC,CADD,EAAqC,CAArC,CAGI,EAFH,WAEG,EADJ,mBAAA,CAAmF,MAAnF,EAAA,WAAA,EAAmF,gBAAA,CAAvD,IAAI,CAAC,UAAkD,CAAA,GAArC,KAAqC,GAAlC,gBAAA,CAAG,IAAI,CAAC,OAAL,CAAa,QAAhB,CAAjD,EAAyE,CAAzE,CACI,CAJN,C,CAdF,E,CAAA,E,WAAA,C;WAmBM,CAnBN,E,GAAA,CAoBI,EAtBN,CA5CM,EAmEN,YAAA,CAAsE,sBAAtE,EAAsE;AAAxD,YAAA,MAAM,EAAE,GAAgD;AAA1C,YAAA,KAAK,EAAE,EAAmC;AAA9B,YAAA,QAAQ,EAAE;AAAoB,WAAtE,CAnEM,CATN;AAAA,S,CADiB;;AAAA,OAAnB,CA8EU,EACV,YAAA,CAAgE,kBAAhE,EAAgE;AAAvD,QAAA,KAAK,EAAC;AAAiD,OAAhE,EAAmB;0BAAE;AAAA,iBAAiC,CAAjC,YAAA,CAAiC,qBAAjC,EAAiC;AAApB,YAAA,EAAE,EAAE,MAAA,CAAA,KAAA,CAAM,IAAN,CAAW;AAAK,WAAjC,E,IAAA,E,CAAA,E,MAAA,CAAiC,CAAjC;AAAA,S,CAAF;;AAAA,OAAnB,CADU,EAEV,YAAA,CAEU,kBAFV,EAEU;AAFD,QAAA,KAAK,EAAC;AAEL,OAFV,EAAmB;0BAAC;AAAA,iBAEpB,C,WAAA,CAFoB;AAAA,S,CAAD;;AAAA,OAAnB,CAFU,CA9EV;AAAA,K;;GADF,E,CAAA,E,UAAA,CAqFI,CAtFN,C","sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <van-tabs v-model:active=\"active\" animated>\r\n      <van-tab title=\"主页\">\r\n        <div class=\"user\">\r\n          <div class=\"data\">\r\n            <p class=\"basic\">基本信息</p>\r\n            <div>村岭:{{ parseInt(attrs.user.createDays / 365) }}年({{ createTime(attrs.user.createTime) }}注册)</div>\r\n            <div>性别:{{ attrs.user.profile.gender === 1 ? '男' : '女' }}</div>\r\n            <div>年龄:{{ `${datalbum(attrs.user.profile.birthday).at(3)}0` }}后</div>\r\n            <div class=\"van-hairline--bottom\"></div>\r\n          </div>\r\n          <div class=\"view\">查看全部<van-icon name=\"arrow\" /></div>\r\n        </div>\r\n        <div class=\"Rank\">\r\n          <div class=\"data\">\r\n            <p class=\"basic\">音乐品味</p>\r\n            <van-cell title=\"听歌排行\" :label=\"`累计听歌${attrs.user.listenSongs}首`\" clickable></van-cell>\r\n            <van-cell\r\n              title=\"我喜欢的音乐\"\r\n              @click=\"\r\n                router.push({\r\n                  name: 'songsdetails',\r\n                  params: {\r\n                    id: like.id,\r\n                  },\r\n                })\r\n              \"\r\n              :label=\"`${like.trackCount}首，播放${like.playCount}次`\"\r\n              clickable\r\n            ></van-cell>\r\n          </div>\r\n        </div>\r\n        <div class=\"ceated ceateds\">\r\n          <div class=\"trackCount gedan\">创建歌单({{ user.length }}个)</div>\r\n          <div\r\n            class=\"item\"\r\n            v-for=\"(item, index) in user\"\r\n            :key=\"index\"\r\n            @click=\"\r\n              router.push({\r\n                name: 'songsdetails',\r\n                params: {\r\n                  id: item.id,\r\n                },\r\n              })\r\n            \"\r\n          >\r\n            <van-image class=\"itemimg\" width=\"40\" height=\"40\" radius=\"15\" :src=\"item.coverImgUrl\" />\r\n            <div class=\"describe\">\r\n              <span class=\"name names\">{{ item.name }}</span\r\n              ><br />\r\n              <span class=\"trackCount\">{{ item.trackCount }} 首 {{ item.creator.nickname }} </span>\r\n              <div class=\"van-hairline--bottom\"></div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"ceated ceateds padding\">\r\n          <div class=\"trackCount gedan\">收藏歌单 ({{ collection.length }}个)</div>\r\n          <div\r\n            class=\"item\"\r\n            v-for=\"(item, index) in collection\"\r\n            :key=\"index\"\r\n            @click=\"\r\n              router.push({\r\n                name: 'songsdetails',\r\n                params: {\r\n                  id: item.id,\r\n                },\r\n              })\r\n            \"\r\n          >\r\n            <van-image class=\"itemimg\" width=\"40\" height=\"40\" radius=\"15\" :src=\"item.coverImgUrl\" />\r\n            <div class=\"describe\">\r\n              <span class=\"name names\">{{ item.name }}</span\r\n              ><br />\r\n              <span class=\"trackCount\">{{ item.trackCount }} 首 {{ item.creator.nickname }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <nut-backtop :bottom=\"150\" :right=\"50\" :duration=\"3000\"></nut-backtop>\r\n      </van-tab>\r\n      <van-tab title=\"动态\"> <UserResuse :id=\"attrs.user.id\"/></van-tab>\r\n      <van-tab title=\"视频\">\r\n        Tab 2\r\n      </van-tab>\r\n    </van-tabs>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { createTime, datalbum } from '@/Util/dayjs.js';\r\nimport { getuserplaylist, getuserevent } from '@/api/user.js';\r\nimport { onMounted, reactive, toRefs, provide, ref } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport UserResuse from './userReuse.vue';\r\nexport default {\r\n  components: {\r\n    UserResuse,\r\n  },\r\n  setup(props, { attrs }) {\r\n    const active = ref(0);\r\n    const playlist = reactive({\r\n      user: [],\r\n      collection: [],\r\n      like: [],\r\n    });\r\n    provide('id', attrs.user.id);\r\n    const router = useRouter();\r\n    onMounted(async () => {\r\n      const { data } = await getuserplaylist({\r\n        uid: attrs.user.id,\r\n      });\r\n      playlist.like = data.playlist[0];\r\n      data.playlist.filter((item) => {\r\n        if (item.userId == attrs.user.id) {\r\n          playlist.user.push(item);\r\n        } else {\r\n          playlist.collection.push(item);\r\n        }\r\n      });\r\n      playlist.user.shift();\r\n      console.log(playlist);\r\n      const res = await getuserevent({\r\n        uid: attrs.user.id,\r\n      });\r\n      console.log(res);\r\n    });\r\n    return {\r\n      active,\r\n      attrs,\r\n      createTime,\r\n      datalbum,\r\n      ...toRefs(playlist),\r\n      router,\r\n      getuserevent,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.content {\r\n  padding: 10px;\r\n  .user,\r\n  .Rank {\r\n    border-radius: 15px;\r\n    background: #fff;\r\n    height: auto;\r\n    padding: 10px;\r\n    margin: 10px 0;\r\n  }\r\n\r\n  .user {\r\n    .data {\r\n      & div {\r\n        margin-top: 5px;\r\n        color: #323233;\r\n        font-size: 13px;\r\n      }\r\n    }\r\n    .view {\r\n      text-align: center;\r\n      color: #323233;\r\n    }\r\n  }\r\n  .Rank {\r\n    height: auto;\r\n  }\r\n  .basic {\r\n    margin: 10px auto;\r\n    font-weight: bold;\r\n    font-size: 15px;\r\n  }\r\n  .data {\r\n    margin: 15px 5px;\r\n  }\r\n  .ceated {\r\n    margin: 10px auto;\r\n    height: auto;\r\n    border-radius: 15px;\r\n    background: #fefefe;\r\n    .item {\r\n      width: 100%;\r\n      display: flex;\r\n      padding: 5px 0;\r\n      .itemimg {\r\n        margin-left: 15px;\r\n      }\r\n    }\r\n  }\r\n  .ceateds {\r\n    background: #fefefe;\r\n\r\n    height: auto;\r\n  }\r\n  .trackCount {\r\n    font-size: 8px;\r\n    color: #bfbfbf;\r\n  }\r\n  .gedan {\r\n    margin: 5px auto;\r\n    padding: 5px;\r\n  }\r\n  .names {\r\n    width: 60vw;\r\n    text-overflow: ellipsis;\r\n    white-space: nowrap;\r\n    overflow: hidden;\r\n  }\r\n  .describe {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-around;\r\n    margin-left: 10px;\r\n    .name {\r\n      font-size: 12px;\r\n    }\r\n  }\r\n  .padding {\r\n    padding-bottom: 200px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}