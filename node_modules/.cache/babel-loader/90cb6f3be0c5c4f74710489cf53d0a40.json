"sources":["D:\\HTML工程\\项目\\v3netease-main\\src\\components\\songsdetails\\components\\playall.vue"],"names":[],"mappings":";;;;;;;;;AA4GO,EAAA,KAAK,EAAC;;;;AAEJ,EAAA,KAAK,EAAC;;;;AAEF,EAAA,KAAK,EAAC;;;AACJ,EAAA,KAAK,EAAC;;;AAEN,EAAA,KAAK,EAAC;;;AACJ,EAAA,KAAK,EAAC;;;AAGN,EAAA,KAAK,EAAC;;;AAKV,EAAA,KAAK,EAAC;;;;;;;;;uBAhBjB,mBAAA,CAoCM,KApCN,EAAA,UAAA,EAoCM,CAnC0D,MAAA,CAAA,IAAA,KAAI,K,kBAAlE,YAAA,CAAgF,sBAAhF,EAAgF;UAAA;AAAnE,IAAA,IAAI,EAAC,SAA8D;AAApD,IAAA,IAAI,EAAC,MAA+C;AAAxC,IAAA,KAAK,EAAC;AAAkC,GAAhF,C,mBACA,mBAAA,CAiCM,KAjCN,EAAA,UAAA,EAiCM,E,kBAhCJ,mBAAA,CA8BM,SA9BN,EA8BM,IA9BN,EA8BM,WAAA,CA9BoC,MAAA,CAAA,QA8BpC,EA9B4C,UAAxB,IAAwB,EAAlB,KAAkB,EAAb;yBAArC,mBAAA,CA8BM,KA9BN,EA8BM;AA9BD,MAAA,KAAK,EAAC,MA8BL;AA9B+C,MAAA,GAAG,EAAE,KA8BpD;AA9B4D,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,MAAA,CAAA,IAAA,CAAK,KAAL,CAAF;AAAA;AA8BjE,KA9BN,E,CACE,mBAAA,CAWM,KAXN,EAAA,UAAA,EAWM,CAVJ,mBAAA,CAAwC,KAAxC,EAAA,UAAA,EAAwC,gBAAA,CAAlB,KAAK,GAAA,CAAa,CAAxC,EAA2B;AAAA;AAA3B,KAUI,EATJ,mBAAA,CAAA,kFAAA,CASI,EARJ,mBAAA,CAOM,KAPN,EAAA,UAAA,EAOM,CANJ,mBAAA,CAEM,KAFN,EAAA,UAAA,EAEM,gBAAA,CADD,IAAI,CAAC,IACJ,CAFN,EACc;AAAA;AADd,KAMI,EAHJ,mBAAA,CAEM,KAFN,EAAA,UAAA,EAEM,gBAAA,WADE,IAAI,CAAC,EAAL,CAAO,CAAP,EAAW,IACb,eADsB,IAAI,CAAC,IAC3B,EAFN,EACqC;AAAA;AADrC,KAGI,CAPN,CAQI,CAXN,C,EAYA,mBAAA,CAgBM,KAhBN,EAAA,UAAA,EAgBM,CAbI,IAAI,CAAC,E,kBAFb,YAAA,CAaE,mBAbF,EAaE;YAAA;AAZA,MAAA,IAAI,EAAC,MAYL;AAVA,MAAA,IAAI,EAAC,IAUL;AATC,MAAA,OAAK,EAAA,cAAA,CAAA,UAAA,MAAA;AAAA,eAAsB,MAAA,CAAA,MAAA,CAAO,IAAP,CAAW;uBAAA;;gBAAkF,IAAI,CAAC;;AAAvF,SAAX,CAAtB;AAAA,OAAA,E,QAAA,CASN;AADA,MAAA,KAAK,EAAC;AACN,KAbF,E,IAAA,E;;AAAA,M,WAAA,C,qCAeI,EADJ,YAAA,CAAsC,mBAAtC,EAAsC;AAA5B,MAAA,IAAI,EAAC,UAAuB;AAAZ,MAAA,IAAI,EAAC;AAAO,KAAtC,CACI,CAhBN,C,CAbF,E;;AAAA,M,UAAA,C;GA8BM,CA9BN,E;;AAAA,GAgCI,GADJ,YAAA,CAAsE,sBAAtE,EAAsE;AAAxD,IAAA,MAAM,EAAE,GAAgD;AAA1C,IAAA,KAAK,EAAE,EAAmC;AAA9B,IAAA,QAAQ,EAAE;AAAoB,GAAtE,CACI,CAjCN,C,CAkCI,CApCN,C","sourcesContent":["<script>\nimport { myMixin } from '@/mixin/index.js';\nimport { playCount, playaudiorule } from '@/Util/fltter.js';\nimport { getallsongs } from '@/api/songsheet.js';\nimport { ref, computed, watchEffect, nextTick } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useStore } from 'vuex';\nimport { Toast } from 'vant';\nimport { show } from '@/hooks/status.js';\nexport default {\n  mixins: [myMixin],\n  name: 'playlistid',\n  setup(props, { attrs, emit }) {\n    const id = ref([]);\n    const allsongs = ref([]);\n    const router = useRouter();\n    const store = useStore();\n    const load = ref(false);\n    watchEffect(async () => {\n      attrs.songsdata.trackIds.forEach((item) => {\n        id.value.push(item.id);\n      });\n      if (ids) {\n        const { data } = await getallsongs({ ids: ids.value.join() });\n        if (data.code === 200) {\n          load.value = true;\n          allsongs.value = data.songs;\n          // console.log(data.songs);\n          emit('length', allsongs.value.length);\n        } else {\n          Toast.fail('加载失败,也许歌曲太多了');\n        }\n      }\n    });\n\n    /**\n     * @param {Number}传入索引\n     */\n    const play = (index) => {\n      playaudiorule(index, store, nextTick, Toast, allsongs.value, show);\n    };\n    const ids = computed(() => {\n      return id.value;\n    });\n    return {\n      allsongs,\n      playCount,\n      play,\n      myMixin,\n      load,\n      router,\n      playaudiorule,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.playall {\n  width: 95vw;\n  height: 65vh;\n  margin: 0 auto;\n\n  .songslist {\n    padding-bottom: 6vh;\n    .item {\n      margin-top: 10px;\n      height: 40px;\n      display: flex;\n      align-items: center;\n      justify-content: space-between;\n      .data {\n        max-width: 40vw;\n        margin-left: 10px;\n        .songname {\n          font-size: 14px;\n          font-weight: bold;\n          margin-bottom: 14px;\n        }\n        .songtitle {\n          font-size: 10px;\n          color: #c4c4c4;\n        }\n      }\n    }\n  }\n  .left {\n    display: flex;\n    .index {\n      line-height: 47.02px;\n      margin-right: 8px;\n      font-size: 12px;\n    }\n  }\n  .loading {\n    position: absolute;\n    left: 50%;\n    top: 50%;\n    transform: translate(-50%, -50%);\n  }\n  .icons {\n    .tv {\n      margin-right: 15px;\n    }\n  }\n}\n</style>\n<template>\n  <div class=\"playall\">\n    <van-loading type=\"spinner\" size=\"80px\" class=\"loading\" v-if=\"load === false\" />\n    <div class=\"songslist auto\" v-else>\n      <div class=\"item\" v-for=\"(item, index) in allsongs\" :key=\"index\" @click=\"play(index)\">\n        <div class=\"left\">\n          <div class=\"index\">{{ index + 1 }}</div>\n          <!-- <van-image width=\"40\" height=\"40\" radius=\"15\" :src=\"item.al.picUrl\" /> -->\n          <div class=\"data\">\n            <div class=\"songname van-ellipsis\">\n              {{ item.name }}\n            </div>\n            <div class=\"songtitle van-ellipsis\">\n              {{ `${item.ar[0].name}--${item.name}` }}\n            </div>\n          </div>\n        </div>\n        <div class=\"icons\">\n          <van-icon\n            name=\"tv-o\"\n            v-if=\"item.mv\"\n            size=\"20\"\n            @click.stop=\"\n              router.push({\n                name: 'video',\n                params: {\n                  id: item.mv,\n                },\n              })\n            \"\n            class=\"tv\"\n          />\n          <van-icon name=\"ellipsis\" size=\"20\" />\n        </div>\n      </div>\n      <nut-backtop :bottom=\"150\" :right=\"50\" :duration=\"3000\"></nut-backtop>\n    </div>\n  </div>\n</template>\n"],"sourceRoot":""}]}