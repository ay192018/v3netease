tup(props, { attrs }) {\n    const album = ref([]);\n    const loading = ref(false);\n    const finished = ref(false);\n    const router = useRouter();\n    const offset = ref(1);\n    const onLoad = async () => {\n      console.log(attrs.attrs.value);\n      const { data } = await getcloudsearch({\n        keywords: attrs.attrs.value,\n        type: 10,\n        offset: (offset.value - 1) * 30,\n      });\n      console.log(data);\n      album.value.push(...data.result.albums);\n      loading.value = false;\n      if (data.result.albums.length) {\n        offset.value++;\n      } else {\n        finished.value = true;\n      }\n    };\n\n    return {\n      attrs,\n      album,\n      onLoad,\n      loading,\n      finished,\n      datalbum,\n      router,\n    };\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.album {\n  background: #fff;\n  padding-bottom: 20px;\n  height: 80vh;\n  overflow-y: auto;\n  .content {\n    width: 60vw;\n    margin-left: 15px;\n    overflow: hidden;\n    height: 100%;\n    .title {\n      margin-right: 4px;\n      vertical-align: middle;\n      text-overflow: ellipsis;\n      white-space: nowrap;\n      display: block;\n\n      width: 80vw;\n    }\n    .describe {\n      font-size: 13px;\n      color: #dedede;\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}