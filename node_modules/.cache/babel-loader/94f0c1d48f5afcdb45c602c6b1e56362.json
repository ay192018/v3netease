          </div>\r\n            <span class=\"user\">{{ item.creator[0].userName }}</span>\r\n            <span class=\"user\"> {{ playCount(item.playTime) }}播放</span>\r\n          </div>\r\n        </template>\r\n      </van-cell>\r\n    </van-list>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getcloudsearch } from \"@/api/search.js\";\r\nimport { ref } from \"vue\";\r\nimport { useRouter } from \"vue-router\";\r\nimport { playCount } from \"@/Util/fltter.js\";\r\nexport default {\r\n  setup(props, { attrs }) {\r\n    const artists = ref([]);\r\n    const loading = ref(false);\r\n    const finished = ref(false);\r\n    const router = useRouter();\r\n    const offset = ref(1);\r\n    const onLoad = async () => {\r\n      // console.log(attrs.attrs.value);\r\n      const { data } = await getcloudsearch({\r\n        keywords: attrs.attrs.value,\r\n        type: 1014,\r\n        offset: (offset.value - 1) * 20,\r\n      });\r\n      console.log(data);\r\n      artists.value.push(...data.result.videos);\r\n      loading.value = false;\r\n\r\n      if (data.result.videos.length) {\r\n        offset.value++;\r\n      } else {\r\n        finished.value = true;\r\n      }\r\n    };\r\n    const tovideo = (vid) => {\r\n      router.push({\r\n        name: \"video\",\r\n        params: {\r\n          id: vid,\r\n        },\r\n      });\r\n    };\r\n    return {\r\n      attrs,\r\n      artists,\r\n      onLoad,\r\n      loading,\r\n      finished,\r\n      router,\r\n      playCount,\r\n      tovideo,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.video {\r\n  background: #fff;\r\n  padding-bottom: 20px;\r\n  height: 80vh;\r\n  overflow-y: auto;\r\n  .content {\r\n    width: 100%-31vw;\r\n    margin-left: 15px;\r\n    .title {\r\n      margin-right: 4px;\r\n      font-size: 13px;\r\n      width: 60vw;\r\n    }\r\n    .describe {\r\n      font-size: 13px;\r\n      color: #dedede;\r\n    }\r\n  }\r\n  .user {\r\n    color: #646566;\r\n    font-size: 12px;\r\n    margin-left: 5px;\r\n  }\r\n  .pic {\r\n    position: relative;\r\n  }\r\n  .count {\r\n    z-index: 1;\r\n    font-size: 12px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}