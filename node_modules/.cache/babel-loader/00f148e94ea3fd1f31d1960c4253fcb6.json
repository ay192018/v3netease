ync (id) => {\r\n      const { data } = await getMlogtovideo({\r\n        id: id,\r\n      });\r\n      router.push({\r\n        name: 'video',\r\n        params: {\r\n          id: data.data,\r\n        },\r\n      });\r\n    };\r\n    return {\r\n      playCount,\r\n      router,\r\n      ref,\r\n      cookie,\r\n      attrs,\r\n      send,\r\n      Videotext,\r\n      recentVideo,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.video {\r\n  height: auto;\r\n  background: #fff;\r\n  padding: 5px;\r\n\r\n  .videolist {\r\n    overflow-x: auto;\r\n    overflow-y: hidden;\r\n    height: auto;\r\n    display: flex;\r\n    .item {\r\n      width: 100px;\r\n      position: relative;\r\n      margin-right: 15px;\r\n      height: auto;\r\n      .titles {\r\n        height: auto;\r\n        width: 100%;\r\n        overflow: hidden;\r\n        font-size: 13px;\r\n      }\r\n    }\r\n  }\r\n}\r\n.bottom-button {\r\n  width: 160px;\r\n  height: 40px;\r\n}\r\n</style>\r\n\r\n<template>\r\n  <div class=\"video auto\">\r\n    <div class=\"recommend\">\r\n      <h3>{{ recentVideo }}</h3>\r\n\r\n      <van-button type=\"primary\" class=\"btn\" hairline round size=\"mini\" @click=\"router.push('/Videosquare')\">\r\n        <template #icon> {{ Videotext }}<van-icon name=\"revoke\" /> </template>\r\n      </van-button>\r\n    </div>\r\n    <div class=\"videolist\">\r\n      <div class=\"item\" v-for=\"(item, index) in attrs.video\" :key=\"index\">\r\n        <van-image\r\n          width=\"110\"\r\n          height=\"130\"\r\n          radius=\"15\"\r\n          fit=\"cover\"\r\n          :src=\"item.resource.mlogBaseData.coverUrl\"\r\n          @click=\"send(item.resource.mlogBaseData.id)\"\r\n        >\r\n          <template v-slot:loading>\r\n            <van-loading type=\"spinner\" size=\"20\" color=\"#000\" />\r\n          </template>\r\n        </van-image>\r\n        <div class=\"titles van-multi-ellipsis--l2\">\r\n          {{ item.resource.mlogBaseData.text }}\r\n        </div>\r\n        <span class=\"count\">{{ playCount(item.resource.mlogExtVO.playCount) }}</span>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <!--  <van-empty image=\"network\" description=\"请登录\" v-else>\r\n    <van-button\r\n      round\r\n      type=\"danger\"\r\n      class=\"bottom-button\"\r\n      @click=\"router.push('/login')\"\r\n      >登录</van-button\r\n    >\r\n  </van-empty> -->\r\n</template>\r\n"],"sourceRoot":""}]}